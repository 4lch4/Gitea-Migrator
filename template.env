GITEA_API_BASE_URL='<GITEA_DOMAIN_NAME>/api/v1'

# Can be generated from the Settings/Applications page:
# https://<GITEA_DOMAIN_NAME>/user/settings/applications
GITEA_API_TOKEN='<GITEA_API_TOKEN>'

# Defaults to https://api.github.com
GH_API_BASE_URL='<GITHUB_API_BASE_URL>'

# Requires a number of different scopes to fully function, see the `IGHConfig` interface for more details.
GH_API_TOKEN='<GITHUB_API_TOKEN>'

# If set to true, the 5s delay when starting the migration process of a repository will be skipped.
# The default value is set to false so that the migration process can be stopped and then restarted in a clean state.
DISABLE_SLEEP=false
